ного реактора.\u003C\u002Fp\u003E","imageUrl":"https:\u002F\u002Fhabrastorage.org\u002Fgetpro\u002Fhabr\u002Fupload_files\u002Fad0\u002F8a4\u002F8b1\u002Fad08a48b1a478dc57b9725b6c7c27807.png","buttonTextHtml":"Читать далее","image":{"url":"https:\u002F\u002Fhabrastorage.org\u002Fgetpro\u002Fhabr\u002Fupload_files\u002Fad0\u002F8a4\u002F8b1\u002Fad08a48b1a478dc57b9725b6c7c27807.png","fit":"cover","positionY":0,"positionX":0}},"status":"published","plannedPublishTime":null,"checked":null,"tags":[{"titleHtml":"simintech"},{"titleHtml":"тау"},{"titleHtml":"ядерный реактор"},{"titleHtml":"передаточные функции"}]},"590671":{"id":"590671","timePublished":"2021-11-22T11:04:33+00:00","isCorporative":true,"lang":"ru","titleHtml":"Видеодетектор огня","editorVersion":"2.0","postType":"article","postLabels":[],"author":{"id":"1879719","alias":"alexn-vorobyev","fullname":"Александр Воробьев","avatarUrl":"\u002F\u002Fhabrastorage.org\u002Fgetpro\u002Fhabr\u002Favatars\u002Fa2c\u002F923\u002F0a6\u002Fa2c9230a64cccf8b568c607f9182b6f3.jpg","speciality":"Computer Vision"},"statistics":{"commentsCount":17,"favoritesCount":19,"readingCount":2620,"score":13,"votesCount":13},"hubs":[{"relatedData":null,"id":"22438","alias":"etmc_exponenta","type":"corporative","title":"Блог компании ЦИТМ Экспонента","titleHtml":"Блог компании ЦИТМ Экспонента","isProfiled":false},{"relatedData":null,"id":"8000","alias":"algorithms","type":"collective","title":"Алгоритмы","titleHtml":"Алгоритмы","isProfiled":true},{"relatedData":null,"id":"19796","alias":"prototyping","type":"collective","title":"Прототипирование","titleHtml":"Прототипирование","isProfiled":true},{"relatedData":null,"id":"21360","alias":"matlab","type":"collective","title":"Matlab","titleHtml":"Matlab","isProfiled":true},{"relatedData":null,"id":"22113","alias":"engineering_systems","type":"collective","title":"Инженерные системы","titleHtml":"Инженерные системы","isProfiled":false}],"flows":[{"id":"1","alias":"develop","title":"Разработка"