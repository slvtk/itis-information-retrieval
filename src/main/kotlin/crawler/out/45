              <article id="654633" data-navigatable="" tabindex="0" class="tm-articles-list__item">
               <div class="tm-article-snippet">
                <div class="tm-article-snippet__meta-container">
                 <div class="tm-article-snippet__meta">
                  <span class="tm-user-info tm-article-snippet__author"><a href="/ru/users/ifap/" title="ifap" class="tm-user-info__userpic">
                    <div class="tm-entity-image">
                     <img alt="" height="24" src="//habrastorage.org/r/w32/getpro/habr/avatars/73f/7bf/34b/73f7bf34bacb0eff84ca4be6e2259c1d.png" width="24" class="tm-entity-image__pic">
