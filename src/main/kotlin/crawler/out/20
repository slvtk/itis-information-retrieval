 взрослых - поработать руками и включить мелкую моторику оказалось неожиданно приятно и полезно.\u003C\u002Fp\u003E\u003Cp\u003EПоехали!\u003C\u002Fp\u003E","imageUrl":"https:\u002F\u002Fhabrastorage.org\u002Fgetpro\u002Fhabr\u002Fupload_files\u002Fa9b\u002F7ae\u002Fe50\u002Fa9b7aee50ffa84c3361374027041a9d6.png","buttonTextHtml":"Читать далее","image":{"url":"https:\u002F\u002Fhabrastorage.org\u002Fgetpro\u002Fhabr\u002Fupload_files\u002Fa9b\u002F7ae\u002Fe50\u002Fa9b7aee50ffa84c3361374027041a9d6.png","fit":"cover","positionY":0,"positionX":0}},"status":"published","plannedPublishTime":null,"checked":null,"tags":[{"titleHtml":"diy"},{"titleHtml":"проектор"},{"titleHtml":"оптика"},{"titleHtml":"эксперимент"},{"titleHtml":"голограмма"},{"titleHtml":"смартфон"},{"titleHtml":"физика детям"},{"titleHtml":"самоделка"},{"titleHtml":"школа"},{"titleHtml":"обучающее видео"}]},"653015":{"id":"653015","timePublished":"2022-02-22T14:33:42+00:00","isCorporative":false,"lang":"ru","titleHtml":"Простой показометр","editorVersion":"2.0","postType":"article","postLabels":[],"author":{"id":"449026","alias":"lesha108","fullname":null,"avatarUrl":"","speciality":""},"statistics":{"commentsCount":10,"favoritesCount":33,"readingCount":7395,"score":21,"votesCount":25},"hubs":[{"relatedData":null,"id":"18961","alias":"rust","type":"collective","title":"Rust","titleHtml":"Rust","isProfiled":true},{"relatedData":null,"id":"19737","alias":"controllers","type":"collective","title":"Программирование микроконтроллеров","titleHtml":"Программирование микроконтроллеров","isProfiled":true},{"relatedData":null,"id":"21976","alias":"DIY","type":"collective","title":"DIY или Сделай сам","titleHtml":"DIY или Сделай сам","isProfiled":false}],"flows":[{"id":"1","alias":"develop","title":"Разработка"},{"id":"7","alias":"popsci","title":"Научпоп"}],"relatedData":null,"leadData":{"textHtml":"\u003Cp\u003EС детства мне нравятся всякие мигающие лампочки на панелях управления, движущиеся стрелки и шкалы приборо